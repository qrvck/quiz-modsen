{
  "presets": [
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],

    "@babel/preset-typescript"
  ],
  "env": {
    "production": {
      "plugins": [
        [
          "react-remove-properties",
          {
            "properties": ["data-cy"],
            "ignoreFilenames": ["node_modules"]
          }
        ]
      ]
    },
    "development": {
      "plugins": []
    }
  }
}
